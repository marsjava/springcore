<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">
	<bean id="car" class="com.spcore.forms.Car">
		<property name="fuelType" value="Petrol"></property>
		<property name="maxSpeed" value="100"></property>
	</bean>
	<bean id="bus" class="com.spcore.forms.Bus">
		<property name="noOfSeats" value="36"></property>
		<property name="maxSpeed" value="80"></property>
	</bean>	
